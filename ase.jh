
application {
  config {
    baseName ase,
    applicationType monolith,
    packageName com.myapp,
    authenticationType jwt,
    prodDatabaseType postgres,
    clientFramework angular
  }
  entities *
}

entity Company {
	uuid UUID 
    name String
    identifier String
    legalName String
    legalInformation String
    externalReference String
    email String
    phone String
    logo String
}
entity Department {
	uuid UUID 
    email String
    phone String
}

